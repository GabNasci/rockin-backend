model supports {
  id         Int       @id @default(autoincrement())
  profile_id Int
  post_id    Int
  profile    profiles  @relation(fields: [profile_id], references: [id])
  post       posts     @relation(fields: [post_id], references: [id])
  created_at DateTime  @default(now())
  updated_at DateTime? @updatedAt
}
