model Support {
  profile_id Int
  post_id    Int
  profile    Profile   @relation(fields: [profile_id], references: [id])
  post       Post      @relation(fields: [post_id], references: [id])
  created_at DateTime  @default(now())
  updated_at DateTime? @updatedAt

  @@id([post_id, profile_id])
  @@map("supports")
}
