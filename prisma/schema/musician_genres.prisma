model MusicianGenre {
  id         Int       @id @default(autoincrement())
  profile_id Int
  genre_id   Int
  profile    Profile   @relation(fields: [profile_id], references: [id])
  genre      Genre     @relation(fields: [genre_id], references: [id])
  created_at DateTime  @default(now())
  updated_at DateTime? @updatedAt

  @@map("musician_genres")
}
