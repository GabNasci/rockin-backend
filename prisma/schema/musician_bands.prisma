model MusicianBand {
  id         Int       @id @default(autoincrement())
  profile_id Int
  band_id    Int
  profile    Profile   @relation(fields: [profile_id], references: [id])
  band       Band      @relation(fields: [band_id], references: [id])
  created_at DateTime  @default(now())
  updated_at DateTime? @updatedAt

  @@map("musician_bands")
}
