model BandGenre {
  id         Int       @id @default(autoincrement())
  band_id    Int
  genre_id   Int
  band       Band      @relation(fields: [band_id], references: [profile_id])
  genre      Genre     @relation(fields: [genre_id], references: [id])
  created_at DateTime  @default(now())
  updated_at DateTime? @updatedAt

  @@map("band_genres")
}
