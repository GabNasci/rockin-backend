model locations {
  id         Int       @id @default(autoincrement())
  latitude   String
  longitude  String
  country    String?
  city       String?
  state      String?
  profile_id Int       @unique
  profile    profiles? @relation(fields: [profile_id], references: [id])
  created_at DateTime  @default(now())
  updated_at DateTime  @updatedAt
}
